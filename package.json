{
  "name": "cuke-helpr",
  "version": "0.1.3",
  "description": "",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tony-kerz/node-cuke-helpr.git"
  },
  "scripts": {
    "cuke": "cross-env DEBUG=test:*,app:* cucumber-js --compiler=js:babel-polyfill --compiler=js:babel-core/register ./test",
    "cuke-feature": "cross-env DEBUG=test:*,app:* cucumber-js --compiler=js:babel-polyfill --compiler=js:babel-core/register",
    "pretest": "xo",
    "test": "npm run cuke",
    "prebuild": "npm run test",
    "build": "babel ./src -d ./dist"
  },
  "author": "tony kerz",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.15.3",
    "config": "^1.24.0",
    "debug": "^2.6.0",
    "helpr": "^0.4.0",
    "lodash": "^4.17.4",
    "test-helpr": "^0.1.10"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-env": "^1.1.8",
    "body-parser": "^1.16.0",
    "cross-env": "^3.1.4",
    "cucumber": "^1.3.1",
    "express": "^4.14.0",
    "xo": "^0.17.1"
  },
  "xo": {
    "space": true,
    "semicolon": false,
    "rules": {
      "no-console": "error",
      "no-unused-expressions": [
        "error",
        {
          "allowShortCircuit": true,
          "allowTernary": true
        }
      ]
    }
  }
}
